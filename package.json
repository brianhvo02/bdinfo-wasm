{
    "name": "bdinfo-wasm",
    "author": "Brian Vo",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@ffmpeg/core": "^0.12.6",
        "@ffmpeg/core-mt": "^0.12.6",
        "@ffmpeg/ffmpeg": "^0.12.10",
        "@ffmpeg/util": "^0.12.1",
        "@mui/material": "^5.15.11",
        "@mui/x-data-grid": "^6.19.5",
        "@reduxjs/toolkit": "^2.2.1",
        "@tanstack/react-table": "^8.12.0",
        "dashjs": "^4.7.4",
        "file-system-access": "^1.0.4",
        "lodash": "^4.17.21",
        "pyodide": "^0.23.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.22.1",
        "react-scripts": "5.0.1",
        "sass": "^1.71.1",
        "typescript": "^4.9.5",
        "wasm-loader": "^1.3.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "wasm-deps": "cp node_modules/pyodide/pyodide.asm.js node_modules/pyodide/python_stdlib.zip node_modules/pyodide/repodata.json node_modules/pyodide/pyodide.asm.wasm node_modules/@ffmpeg/core-mt/dist/esm/* public/static/js",
        "local:build": "cd wasm && sh ./scripts/build.sh && sh ./scripts/compile.sh",
        "local:compile": "cd wasm && sh ./scripts/compile.sh",
        "local:cleanup": "rm -rf wasm/build public/static/js/* public/static/py/*.whl build",
        "docker:build": "sh ./wasm/scripts/docker_build.sh",
        "docker:compile": "sh ./wasm/scripts/docker_compile.sh"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "ignorePatterns": [
            "src/libbluray.js"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/emscripten": "^1.39.10",
        "@types/lodash": "^4.14.202",
        "@types/node": "^16.18.83",
        "@types/react": "^18.2.58",
        "@types/react-dom": "^18.2.19",
        "@types/wicg-file-system-access": "^2023.10.4"
    }
}
